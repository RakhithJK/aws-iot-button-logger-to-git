  "scripts": {
    "build": "ts-node src/build.ts",
    "fix": "npm-run-all --continue-on-error fix:*",
    "fix:markdownlint": "markdownlint --fix \"**/*\"",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-deduplicate": "yarn install && yarn-deduplicate --strategy=fewer && yarn install",
    "lint": "npm-run-all --continue-on-error lint:*",
    "lint:markdownlint": "markdownlint \"**/*\"",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "tsc --project .",
    "lint:yarn-deduplicate": "yarn-deduplicate --fail --list --strategy=fewer",
    "simulate": "ts-node src/simulate.ts",
    "test": "jest"
  },
  "dependencies": {
    "@kachkaev/commands": "^0.1.0",
    "@types/jest": "^26.0.13",
    "envalid": "^6.0.2",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.1",
    "jszip": "^3.5.0",
    "lambda-git": "^0.1.2",
    "luxon": "^1.25.0",
    "parcel-bundler": "^1.12.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@kachkaev/markdownlint-config": "^0.1.0",
    "@kachkaev/prettier-config": "^1.0.0",
    "@types/aws-lambda": "^8.10.62",
    "@types/fs-extra": "^9.0.1",
    "@types/jszip": "^3.4.1",
    "@types/luxon": "^1.24.4",
    "@types/node": "^14.6.4",
