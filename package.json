    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "markdownlint-cli": "^0.23.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "prettier-plugin-packagejson": "^2.2.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "yarn-deduplicate": "^3.0.0"
  },
  "engines": {
    "node": ">=10.21.0",
